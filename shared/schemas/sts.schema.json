      "minimum": 1,
      "maximum": 110,
      "examples": [68, 55, 72],
      "medicalTerms": ["age", "years old", "yo"],
      "fieldId": "ageN"
    },
    "gender": {
      "type": "string",
      "title": "Gender",
      "description": "Biological sex",
      "enum": ["Male", "Female"],
      "examples": ["Male", "Female"],
      "medicalTerms": ["gender", "sex", "male", "female", "M", "F"],
      "fieldId": "gender"
    },
    "height_cm": {
      "type": "number",
      "title": "Height",
      "description": "Height in centimeters",
      "minimum": 20,
      "maximum": 251,
      "examples": [170.5, 165.0, 182.0],
      "unit": "cm",
      "medicalTerms": ["height", "cm", "centimeters"],
      "fieldId": "heightN"
