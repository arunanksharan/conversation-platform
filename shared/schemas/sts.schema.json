{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://healthcare-conversation-platform.dev/schemas/sts-risk.json",
  "title": "STS Adult Cardiac Surgery Risk Calculator",
  "description": "Society of Thoracic Surgeons (STS) Adult Cardiac Surgery Database risk model for predicting operative mortality and morbidity. Schema updated with live field extraction from https://acsdriskcalc.research.sts.org/",
  "type": "object",
  "required": [
