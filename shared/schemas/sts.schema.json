          "Black/African American",
          "Asian",
          "Am Indian/Alaskan",
          "Hawaiian/Pacific Islander",
          "Hispanic",
          "Other"
        ]
      },
      "examples": [["White"], ["Black/African American"], ["Asian", "Hispanic"]],
      "medicalTerms": ["race", "ethnicity"],
      "fieldId": "racemulti"
